<%- include('../../partials/header.ejs')%>
<%- include('../../partials/navbar.ejs')%>

<div class="container">
    <hr>
    <div class="card">

        <div class="card-header">
            <h2>Edição de categoria: <%=testes.nome%>
            </h2>
        </div>

        <div class="card-body">


            <form action="/admin/articles/newa/edit/save" method="post">
                <input class="form-control" type="text" name="nome" placeholder="Defina o titulo da categoria"
                    value="<%=testes.nome%>">
                <input type="hidden" name="id" value="<%= testes.id%>">
                <br>
                <button class="btn btn-success" type="submit">Atualizar</button>
            </form>
        </div>
    </div>
</div>

<%- include('../../partials/footer.ejs')%>